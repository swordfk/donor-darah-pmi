<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.example.donordarah">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DonorDarah"
        android:usesCleartextTraffic="true">
        <activity android:name="id.example.ViewPendonor2"></activity>
        <activity android:name="id.example.ViewPendonor" />
        <activity android:name=".DaftarKPMI" />
        <activity android:name="id.example.DetailKPMI2" />
        <activity android:name=".ViewKPMIUser" />
        <activity android:name=".MainActivity2" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name="id.example.AddKPMI" />
        <activity android:name="id.example.DetailKMI" />
        <activity android:name=".TambahJadwal" />
        <activity android:name=".ViewKPMI" />
        <activity android:name=".TambahKPMI" />
        <activity android:name=".TambahDarah" />
        <activity android:name=".ViewDarah" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>